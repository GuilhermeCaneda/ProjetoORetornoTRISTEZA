<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_degrade"
    tools:context=".controle.EditarContatosProfessor">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/whatsappEditarContatosProfessor"
        app:layout_constraintEnd_toStartOf="@+id/whatsappEditarContatosProfessor"
        app:layout_constraintTop_toTopOf="@+id/whatsappEditarContatosProfessor"
        app:srcCompat="@drawable/ic_whatsapp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/emailEditarContatosProfessor"
        app:layout_constraintEnd_toStartOf="@+id/emailEditarContatosProfessor"
        app:layout_constraintTop_toTopOf="@+id/emailEditarContatosProfessor"
        app:srcCompat="@drawable/ic_gmail" />

    <EditText
        android:id="@+id/instagramEditarContatosProfessor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="72dp"
        android:layout_marginRight="72dp"

        android:ems="10"
        android:hint="Instagram"
        android:inputType="textPersonName"
        android:textColor="#C8C7C7"
        android:textColorHint="#C8C7C7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/emailEditarContatosProfessor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Email"
        android:textColor="#C8C7C7"
        android:textColorHint="#C8C7C7"
        app:layout_constraintEnd_toEndOf="@+id/instagramEditarContatosProfessor"
        app:layout_constraintTop_toBottomOf="@+id/instagramEditarContatosProfessor" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/instagramEditarContatosProfessor"
        app:layout_constraintEnd_toStartOf="@+id/instagramEditarContatosProfessor"
        app:layout_constraintTop_toTopOf="@+id/instagramEditarContatosProfessor"
        app:srcCompat="@drawable/ic_instagram" />

    <EditText
        android:id="@+id/whatsappEditarContatosProfessor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="Whatsapp"
        android:inputType="textPersonName"
        android:textColor="#C8C7C7"
        android:textColorHint="#C8C7C7"
        app:layout_constraintEnd_toEndOf="@+id/emailEditarContatosProfessor"
        app:layout_constraintTop_toBottomOf="@+id/emailEditarContatosProfessor" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <Button
        android:id="@+id/salvarEditarContatosProfessor"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:background="@drawable/bg_button"
        android:text="Salvar"
        android:textColor="#C8C7C7"
        app:layout_constraintEnd_toEndOf="@+id/whatsappEditarContatosProfessor"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/whatsappEditarContatosProfessor" />

</androidx.constraintlayout.widget.ConstraintLayout>